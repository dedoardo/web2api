{
    "manifest_version": 2,
    "name": "web2api",
    "version": "0.9",
    "minimum_chrome_version": "18.0",
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "devtools_page" : "panel_redirect.html",
    "permissions": [
          "downloads",
          "tabs"
        ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
